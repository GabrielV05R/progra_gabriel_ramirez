#include <iostream>
#include <conio.h>

using namespace std;
int main() {
	cout<<"Ingresa las 10 edades"<<endl;
	
	int edad[10];
	for(int i=0; i<10; i++){
		cin>>edad[i];
	}
	for(int m=0; m<10; m++){
		cout<<edad[m]<<" ";
	}	
	int aux;
	for(int k=0; k<10; k++){
		for(int j=0; j<10; j++){
		
		if(edades[j]>edad[j+1]){
			aux=edad[j];
			edad[j]=edad[j+1];
			edades[j+1]=aux;
			
	}
	}
	}
	
	cout<<endl;
	cout<<"vector ordenado"<<endl;
	for(int a=0; a<10; a++){
	cout<<edad[a]<<" ";
	}
	
	getch();
	
	return 0;
}
